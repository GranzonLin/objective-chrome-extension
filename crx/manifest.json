{
  "name": "行事易",
  "version": "1.0",
  "manifest_version": 2,
  "description": "今日待办列表",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://api.asfun.cn/", "https://api.exease.com/",
    "alarms", "tabs", "notifications"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": {
      "16": "icon_16.png",
      "24": "icon_24.png",
      "32": "icon_32.png"
    },
    "default_popup": "page/popup.html"
  }
}
